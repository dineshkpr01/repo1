{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredf-learning-01"
		},
		"LS_SQL_AZURE_SQL_AUTH_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_AZURE_SQL_AUTH'"
		},
		"LS_SQL_AZURE_MANAGED_IDENTITY_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_AZURE_MANAGED_IDENTITY'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=lazuresqlserver.database.windows.net;Initial Catalog=azuresqldb01"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredf-learning-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_AZURE_SQL_AUTH_connectionString": {
						"value": "[parameters('LS_SQL_AZURE_SQL_AUTH_connectionString')]"
					},
					"LS_SQL_AZURE_MANAGED_IDENTITY_connectionString": {
						"value": "[parameters('LS_SQL_AZURE_MANAGED_IDENTITY_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}